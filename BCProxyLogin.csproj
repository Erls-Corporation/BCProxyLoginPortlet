if "$(ConfigurationName)" == "Portal-dev" goto :PortalDev
if "$(ConfigurationName)" == "Portal" goto :Portal


:PortalDev

XCOPY "$(TargetDir)Portal" "V:\program files\jenzabar\ics.net\portal" /E /Y
